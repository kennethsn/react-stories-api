{
  "name": "react-stories-api",
  "version": "0.0.1-dev-8",
  "description": "React Components for building interactive data storytelling interfaces with the Stories API",
  "author": "kennethsn",
  "license": "MIT",
  "repository": "kennethsn/react-stories-api",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "jsnext:main": "dist/index.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "styleguide-server": "styleguidist server",
    "styleguide-build": "NODE_ENV=production styleguidist build",
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "start": "./node_modules/.bin/parcel src/docs/index.html",
    "build": "rm -rf dist && NODE_ENV=production babel src --out-dir dist --copy-files --ignore __tests__,spec.js,test.js,__snapshots__,Readme.md",
    "prepare": "npm run build",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.7.0",
    "prop-types": "^15.7.2",
    "react": "^15.0.0 || ^16.12.0",
    "react-dom": "^15.0.0 || ^16.12.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.2",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.0",
    "@svgr/rollup": "^2.4.1",
    "acorn": "^6.3.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.1.4",
    "css-loader": "^3.2.0",
    "eslint": "^6.6.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.1.1",
    "node-sass": "^4.13.0",
    "parcel-bundler": "^1.12.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "^3.2.0",
    "react-styleguidist": "^10.2.1",
    "rollup": "^0.64.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-scss": "^1.0.2",
    "rollup-plugin-url": "^1.4.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^1.7.2"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "axios": "^0.19.0",
    "material-ui-audio-player": "^1.0.6",
    "mirador": "^3.0.0-alpha.16",
    "react-awesome-slider": "^2.2.1",
    "react-draggable": "^4.1.0",
    "react-icons": "^3.8.0",
    "react-map-gl": "^5.1.3",
    "react-masonry-component": "^6.2.1",
    "react-multi-carousel": "^2.4.2",
    "react-paginate": "^6.3.2",
    "react-parallax": "^2.2.4",
    "react-vertical-timeline-component": "^2.5.0"
  }
}
