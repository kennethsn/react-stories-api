{
  "name": "react-stories-api",
  "version": "1.0.0-dev-0",
  "author": "Kenneth Seals-Nutt",
  "babel": {
    "presets": [
      "@babel/preset-env",
      [
        "@babel/preset-react",
        {
          "runtime": "automatic"
        }
      ],
      "@babel/preset-typescript"
    ],
    "sourceType": "unambiguous"
  },
  "bugs": {
    "url": "https://github.com/kennethsn/react-stories-api/issues"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "description": "React Components for building interactive data storytelling interfaces with the Stories API",
  "devDependencies": {
    "@babel/preset-env": "^7.25.0",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@chromatic-com/storybook": "^1.6.1",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/cz-commitlint": "^19.2.0",
    "@commitlint/prompt-cli": "^19.3.1",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@storybook/addon-essentials": "^8.2.6",
    "@storybook/addon-interactions": "^8.2.6",
    "@storybook/addon-links": "^8.2.6",
    "@storybook/addon-onboarding": "^8.2.6",
    "@storybook/blocks": "^8.2.6",
    "@storybook/react": "^8.2.6",
    "@storybook/react-vite": "^8.2.6",
    "@storybook/test": "^8.2.6",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.3.3",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "commitizen": "^4.3.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.9",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^9.1.3",
    "inquirer": "^9.3.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-postcss": "^4.0.2",
    "standard-version": "^9.5.0",
    "storybook": "^8.2.6",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/kennethsn/react-stories-api#readme",
  "jest": {
    "testEnvironment": "jsdom"
  },
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "peerDependencies": {
    "react": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kennethsn/react-stories-api.git"
  },
  "scripts": {
    "build": "rollup  -c",
    "build-storybook": "storybook build",
    "commit": "git-cz",
    "dev": "rollup -c -w",
    "lint": "eslint . --ext cjs,js,jsx,mjs,ts,tsx",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "husky",
    "storybook": "storybook dev -p 6006",
    "release": "standard-version",
    "test": "jest",
    "test:watch": "npm test -- --watchAll --verbose"
  },
  "types": "dist/index.d.ts"
