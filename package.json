{
  "name": "react-stories-api",
  "version": "1.0.0-dev-5",
  "author": "Kenneth Seals-Nutt",
  "babel": {
    "presets": [
      "@babel/preset-env",
      [
        "@babel/preset-react",
        {
          "runtime": "automatic"
        }
      ],
      "@babel/preset-typescript"
    ],
    "sourceType": "unambiguous"
  },
  "bugs": {
    "url": "https://github.com/kennethsn/react-stories-api/issues"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "description": "React Components for building interactive data storytelling interfaces with the Stories API",
  "dependencies": {
    "@fontsource/roboto": "^5.1.0",
    "react-if": "^4.1.5",
    "react-photo-view": "^1.2.6",
    "react-resizable-panels": "^2.1.4",
    "swiper": "^11.1.14"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.25.7",
    "@babel/preset-react": "^7.25.7",
    "@babel/preset-typescript": "^7.25.7",
    "@chromatic-com/storybook": "^1.8.0",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/cz-commitlint": "^19.5.0",
    "@commitlint/prompt-cli": "^19.5.0",
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-typescript": "^11.1.6",
    "@storybook/addon-essentials": "^8.3.5",
    "@storybook/addon-interactions": "^8.3.5",
    "@storybook/addon-links": "^8.3.5",
    "@storybook/addon-onboarding": "^8.3.5",
    "@storybook/blocks": "^8.3.5",
    "@storybook/react": "^8.3.5",
    "@storybook/react-vite": "^8.3.5",
    "@storybook/test": "^8.3.5",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@types/jest": "^29.5.13",
    "@types/react": "^18.3.11",
    "@types/rollup-plugin-peer-deps-external": "^2.2.5",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "commitizen": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.12",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^9.1.6",
    "inquirer": "^9.3.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "relative-deps": "^1.0.7",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "standard-version": "^9.5.0",
    "storybook": "^8.3.5",
    "storybook-addon-deep-controls": "^0.8.0",
    "tslib": "^2.7.0",
    "typescript": "^5.6.2"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/kennethsn/react-stories-api#readme",
  "jest": {
    "testEnvironment": "jsdom",
    "transformIgnorePatterns": [
      "node_modules/(?!swiper)/"
    ]
  },
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "peerDependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.3",
    "@mui/lab": "^6.0.0-beta.11",
    "@mui/material": "^6.1.3",
    "react": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kennethsn/react-stories-api.git"
  },
  "scripts": {
    "build": "rollup  -c",
    "build-storybook": "storybook build",
    "commit": "git-cz",
    "dev": "rollup -c -w",
    "lint": "eslint . --ext cjs,js,jsx,mjs,ts,tsx",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "husky",
    "storybook": "storybook dev -p 6006",
    "release": "standard-version",
    "test": "jest",
    "test:watch": "npm test -- --watchAll --verbose"
  },
  "types": "dist/index.d.ts"
}
